{
  "name": "pull-request-limiter",
  "version": "1.0.0",
  "description": "GitHub app to limit pull requests",
  "author": "Amol Grover <frextrite@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com//pull-request-limiter.git",
  "homepage": "https://github.com//pull-request-limiter",
  "bugs": "https://github.com//pull-request-limiter/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^9.5.3",
    "acorn": "5.7.4",
    "yargs-parser": "13.1.2",
    "node-fetch": "2.6.1",
    "ini": "1.3.8",
    "y18n": "4.0.1",
    "lodash": "4.17.21",
    "hosted-git-info": "2.8.9",
    "path-parse": "1.0.7",
    "tmpl": "1.0.5",
    "url-parse": "1.5.8"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nock": "^11.4.0",
    "nodemon": "^1.19.4",
    "smee-client": "^1.1.0",
    "standard": "^14.3.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
